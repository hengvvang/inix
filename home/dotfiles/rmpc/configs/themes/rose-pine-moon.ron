#![enable(implicit_some)]
#![enable(unwrap_newtypes)]
#![enable(unwrap_variant_newtypes)]
(
    // Rose Pine Moon 主题配置 - 中等对比度变体
    // 基于 Rose Pine Moon 配色方案 - https://rosepinetheme.com/
    
    default_album_art_path: None,
    show_song_table_header: true,
    draw_borders: true,
    modal_backdrop: true,
    format_tag_separator: " • ",
    browser_column_widths: [20, 38, 42],
    
    // Rose Pine Moon 核心颜色配置
    background_color: "#232136",        // Base - 深蓝紫色背景
    text_color: "#e0def4",             // Text - 主文本颜色
    header_background_color: "#2a273f", // Surface - 头部背景
    modal_background_color: "#393552",   // Overlay - 模态框背景
    
    // 预览和标签样式
    preview_label_style: (fg: "#f6c177", modifiers: "Bold"),           // Gold - 预览标签
    preview_metadata_group_style: (fg: "#c4a7e7", modifiers: "Bold"), // Iris - 元数据组
    
    // 标签栏配置 - Moon 风格
    tab_bar: (
        enabled: true,
        active_style: (fg: "#232136", bg: "#c4a7e7", modifiers: "Bold"),    // 活动标签
        inactive_style: (fg: "#908caa"),                                     // 非活动标签
    ),
    
    // 进度条样式 - Moon 粉色系
    progress_bar: (
        symbols: ProgressBarSymbols(
            left_end: "▐",
            right_end: "▌",
            filled: "█",
            empty: "─",
        ),
        style: (fg: "#ea9a97"),              // Rose - Moon 粉色
        buffered_style: (fg: "#f6c177"),     // Gold - 缓冲区
    ),
    
    // 音量控制样式
    volume_bar: (
        symbols: VolumeBarSymbols(
            left_end: "▐",
            right_end: "▌", 
            filled: "█",
            empty: "─",
        ),
        style: (fg: "#c4a7e7"),              // Iris - Moon 紫色
    ),
    
    // 播放状态指示器
    playback_symbols: PlaybackSymbols(
        playing: "▶",
        paused: "⏸", 
        stopped: "⏹",
    ),
    
    // 播放状态样式
    playing_style: (fg: "#eb6f92", modifiers: "Bold"),     // Love - 播放状态
    paused_style: (fg: "#908caa"),                         // Subtle - 暂停状态
    stopped_style: (fg: "#6e6a86"),                        // Muted - 停止状态
    
    // 边框样式
    borders: Borders(
        top_left: "╭", top_right: "╮",
        bottom_left: "╰", bottom_right: "╯", 
        horizontal: "─", vertical: "│", cross: "┼",
        vertical_left: "┤", vertical_right: "├",
        horizontal_top: "┬", horizontal_bottom: "┴",
    ),
    
    border_style: (fg: "#393552"),
    selected_border_style: (fg: "#c4a7e7", modifiers: "Bold"),
    
    // 搜索和表格样式
    search_match_style: (bg: "#f6c177", fg: "#232136", modifiers: "Bold"),
    table_header_style: (fg: "#c4a7e7", modifiers: "Bold"),
    table_row_style: (fg: "#e0def4"),
    table_selected_style: (bg: "#393552", fg: "#c4a7e7"),
    
    // 快捷键和队列样式
    shortcut_style: (fg: "#f6c177", modifiers: "Bold"),
    shortcut_description_style: (fg: "#908caa"),
    queue_title_style: (fg: "#c4a7e7", modifiers: "Bold"),
    queue_current_song_style: (bg: "#2a273f", fg: "#ea9a97", modifiers: "Bold"),
    
    // 占位符和滚动条
    album_art_placeholder_style: (fg: "#6e6a86"),
    scrollbar_track_style: (fg: "#393552"),
    scrollbar_thumb_style: (fg: "#c4a7e7"),
    
    // 状态样式
    error_style: (fg: "#eb6f92", modifiers: "Bold"),
    warning_style: (fg: "#f6c177", modifiers: "Bold"),
    info_style: (fg: "#9ccfd8", modifiers: "Bold"),
    success_style: (fg: "#3e8fb0", modifiers: "Bold"),
    
    // 文本样式
    secondary_text_style: (fg: "#908caa"),
    disabled_text_style: (fg: "#6e6a86"),
    list_item_style: (fg: "#e0def4"),
    list_item_selected_style: (bg: "#393552", fg: "#c4a7e7"),
    list_item_focused_style: (bg: "#2a273f", fg: "#ea9a97"),
    
    // 输入框样式
    input_style: (fg: "#e0def4", bg: "#2a273f"),
    input_focused_style: (fg: "#e0def4", bg: "#393552"),
    input_cursor_style: (fg: "#c4a7e7", modifiers: "Bold"),
    
    // 对话框样式
    dialog_border_style: (fg: "#c4a7e7"),
    dialog_title_style: (fg: "#f6c177", modifiers: "Bold"),
    
    // 状态栏样式
    status_bar_background_style: (bg: "#2a273f"),
    status_bar_text_style: (fg: "#e0def4"),
    
    // 帮助文本样式
    help_key_style: (fg: "#c4a7e7", modifiers: "Bold"),
    help_description_style: (fg: "#908caa"),
    
    // 时间显示样式
    time_style: (fg: "#f6c177"),
    time_remaining_style: (fg: "#908caa"),
    
    // 标签样式
    tag_style: (fg: "#9ccfd8"),
    tag_value_style: (fg: "#e0def4"),
    
    // 文件浏览器样式
    directory_style: (fg: "#c4a7e7", modifiers: "Bold"),
    file_style: (fg: "#e0def4"),
    audio_file_style: (fg: "#ea9a97", modifiers: "Bold"),
    
    // 播放列表样式
    playlist_style: (fg: "#9ccfd8", modifiers: "Bold"),
    playlist_current_style: (fg: "#f6c177", modifiers: "Bold"),
    
    // 艺术家和专辑样式
    artist_style: (fg: "#c4a7e7", modifiers: "Bold"),
    album_style: (fg: "#ea9a97"),
    song_title_style: (fg: "#e0def4", modifiers: "Bold"),
    
    // 统计信息样式
    stats_label_style: (fg: "#908caa"),
    stats_value_style: (fg: "#f6c177", modifiers: "Bold"),
    
    // Logo 和标题样式
    logo_style: (fg: "#ea9a97", modifiers: "Bold"),
    app_title_style: (fg: "#c4a7e7", modifiers: "Bold"),
    
    // 分隔符样式
    separator_style: (fg: "#393552"),
    
    // 加载指示器样式
    loading_style: (fg: "#c4a7e7", modifiers: "Bold"),
    
    // 空状态样式
    empty_state_style: (fg: "#6e6a86"),
)
